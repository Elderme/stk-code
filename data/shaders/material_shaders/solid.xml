<!--
Containt the solid material shader.
This material has one of the most basic shaders avaliable
-->
 
 <material_shaders>
    <!-- Configuration flags -->
    <flags
        unlite="false"
        cast_shadow="true"
        normal_map="false"
        alpha_ref="false"
        wind_sensitive="false"
    />
    
    <rendering_passes>
        <solid_first_pass>
            <vertex_shader code="generic">
            <fragment_shader code="generic">
        </solid_first_pass>
        
        <solid_second_pass>
            <vertex_shader code="generic">
            <fragment_shader code="generic">
        </solid_second_pass>
        
        <shadow_pass>
            <vertex_shader code="generic">
            <geometry_shader code="generic">
            <fragment_shader code="generic">
        </shadow_pass>
        
    </rendering_passes>

</material_shaders>
