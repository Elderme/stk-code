<!--
Containt the solid material shader.
This material has one of the most basic shaders avaliable
-->
 
 <material_shaders>
    <!-- Configuration flags -->
    <flags
        unlite="false"
        cast_shadow="true"
        normal_map="false"
        alpha_ref="false"
        wind_sensitive="false"
    />
    
    <rendering_passes>
        <solid_first_pass
            use_generic_shader="true"
        />
        <solid_second_pass
            use_generic_shader="true"
        />
        <shadow_pass
            use_generic_shader="true"
        />        
    </rendering_passes>

</material_shaders>
